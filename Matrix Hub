local TweenService = game:GetService("TweenService")

local BulletTracesEnabled = false

local function cloneRayPart(rayPart)
    if not BulletTracesEnabled then return end
    if not rayPart:IsA("Part") then return end
    local clone = rayPart:Clone()
    clone.Parent = workspace
    clone.Anchored = rayPart.Anchored
    clone.CanCollide = false
    clone.CanQuery = false
    clone.CanTouch = false
    clone.Color = Color3.fromRGB(200,128,255)
    clone.Transparency = 0.45
    local tween = TweenService:Create(clone, TweenInfo.new(0.45, Enum.EasingStyle.Linear), {Transparency = 1})
    tween:Play()
    tween.Completed:Connect(function() clone:Destroy() end)
end

local function monitorContainer(container)
    container.DescendantAdded:Connect(function(d)
        if d:IsA("Part") and d.Name == "RayPart" then
            cloneRayPart(d)
        end
    end)
    for _, v in ipairs(container:GetChildren()) do
        if v:IsA("Folder") or v:IsA("Model") then
            monitorContainer(v)
        end
    end
end

monitorContainer(workspace)
if game:FindFirstChild("Camera") then
    monitorContainer(game.Camera)
end

