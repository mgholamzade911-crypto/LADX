    Name = "Bullet Traces",
    CurrentValue = false,
    Flag = "BulletTraces_Toggle",
    Callback = function(Value)
        BulletTracesEnabled = Value
        showFrame("Bullet Traces " .. (Value and "Enabled" or "Disabled"))
        task.delay(1.5, hideFrame)
    end
})

